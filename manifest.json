{
    "manifest_version": 3,
    "name": "Disable Blur",
    "version": "1.0",
    "description": "Disable all blurred text on a webpage.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  